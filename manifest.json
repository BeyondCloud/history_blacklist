{
    "background": {
        "scripts": [
            "default_blacklist.js",
            "checkURL.js"
        ]
    },
    "description": "Cleans your History from Sites after visiting by keywords",
    "icons": {
        "128": "icon.png",
        "16": "icon.png",
        "48": "icon.png"
    },
    "manifest_version": 2,
    "name": "Browser History Filter",
    "options_page": "popup.html",
    "permissions": [
        "history",
        "tabs",
        "http://*/*",
        "background",
        "storage"
    ],
    "update_url": "https://clients2.google.com/service/update2/crx",
    "version": "1.2.1"
}